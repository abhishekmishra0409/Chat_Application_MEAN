{
  "version": 2,
  "builds": [{
    "src": "dist/frontend/**/*",
    "use": "@vercel/static"
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "/dist/frontend/index.html",
    "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
  }],
  "rewrites": [{
    "source": "/(.*)",
    "destination": "/index.html"
  }]
}
