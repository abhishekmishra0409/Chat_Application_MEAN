{
  "version": 2,
  "builds": [{
    "src": "dist/browser/**/*",
    "use": "@vercel/static"
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "/dist/browser/index.html"
  }],
  "headers": [{
      "source": "/(.*)",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=3600"
      }]
    },
    {
      "source": "assets/(.*)",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=31536000, immutable"
      }]
    },
    {
      "source": "*.js",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=31536000, immutable"
      }]
    },
    {
      "source": "*.css",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=31536000, immutable"
      }]
    }
  ]
}
